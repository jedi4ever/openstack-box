--cc_host=<%= node.pub %>
--dhcpbridge=/usr/bin/nova-dhcpbridge
--dhcpbridge_flagfile=/etc/nova/nova.conf
--fixed_range=192.168.0.0/16
--glance_api_servers=<%= node.prv %>:9292
--image_service=nova.image.glance.GlanceImageService
--iscsi_ip_prefix=192.168.
--lock_path=/var/lock/nova
--logdir=/var/log/nova
--network_size=8
--nova_url=http://<%= node.pub %>:8774/v1.1/
--rabbit_host=<%= node.pub %>
--routing_source_ip=<%= node.pub %>
--s3_host=<%= node.pub %>
--sql_connection=mysql://<%= node.nova.mysql.user %>:<%= node.nova.mysql.password %>@<%= node.nova.mysql.host %>/<%= node.nova.mysql.database %>
--state_path=/var/lib/nova
--use_deprecated_auth
--verbose
--vlan_interface=br100
